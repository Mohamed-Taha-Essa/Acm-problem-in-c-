   "source": [
    "\n",
    "number =int(input())\n",
    "summ =0\n",
    "\n",
    "m =[]\n",
    "n =[]\n",
    "for _ in range(number):\n",
    "        a , b =map(int, input().split())\n",
    "        m.append(a)\n",
    "        n.append(b)\n",
    "\n",
    "for i in range(0,number):\n",
    "    for j in range(0,number):\n",
    "        if(m[i] == n[j]):\n",
    "            summ+=1\n",
    "            \n",
    "print(summ)"
   ]
